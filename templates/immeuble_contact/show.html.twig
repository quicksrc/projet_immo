{% extends 'base.html.twig' %}

{% block title %}ImmeubleContact
{% endblock %}

{% block body %}
	<div id="myTabContent" class="tab-content">
		<div class="tab-pane fade active show" id="home" role="tabpanel">
			<div class="accordion" id="accordionExample">
				<div class="accordion-item">
					<h2 class="accordion-header" id="headingOne">
						<button class="accordion-button collapsed fw-bold" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
							Immeuble Contact
						</button>
					</h2>
					<div id="collapseOne" style="padding:2%" class="accordion-collapse collapse show" aria-labelledby="headingOne" data-bs-parent="#accordionExample" style="">
						<div class="accordion-body">
							<div class="form-group row">
								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">ID Immeuble Contact</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.IDImmeubleContact }}" readonly="">
								</div>
							</div>
							<div class="form-group row">
								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">Qualité</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.Qualite }}" readonly="">
								</div>

								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">Qualité Proprietaire</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.QualiteProprietaire }}" readonly="">
								</div>
							</div>
							<div class="form-group row">
								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">Genre</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.Genre}}" readonly="">
								</div>

								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">Principal</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.Principal }}" readonly="">
								</div>
							</div>
							<div class="form-group row">
								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">Ne Vend Pas</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.NeVendPas}}" readonly="">
								</div>

								<div class="col-md-4 mb-3">
									<label for="readOnlyInput">Date NVP</label>
									<input class="form-control" id="readOnlyInput" type="text" placeholder="{{ immeuble_contact.DateNVP ? immeuble_contact.DateNVP|date('Y-m-d H:i:s') : '' }}" readonly="">
								</div>
							</div>

						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<br>
	<div class="float-end ">
		<button class="btn btn-light no-print" type="button">
			<a class=" text-decoration-underline-remove" href="javascript:history.go(-1)">Retourner à la liste</a>
		</button>
		{# <button class="btn btn-light no-print" type="button">
											<a class=" text-decoration-underline-remove" href="{{ path('immeuble_contact_edit', {'IDImmeubleContact': immeuble_contact.IDImmeubleContact}) }}">edit</a>
										</button> #}
	</div>

	{{ include('immeuble_contact/_delete_form.html.twig') }}
{% endblock %}
