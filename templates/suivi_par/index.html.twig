{% extends 'base.html.twig' %}

{% block title %}SuiviPar index
{% endblock %}

{% block body %}
	<h1>SuiviPar index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>IDSuiviPar</th>
				<th>Libelle</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for suivi_par in suivi_pars %}
				<tr>
					<td>{{ suivi_par.IDSuiviPar }}</td>
					<td>{{ suivi_par.Libelle }}</td>
					<td>
						<a href="{{ path('suivi_par_show', {'IDSuiviPar': suivi_par.IDSuiviPar}) }}">show</a>
						<a href="{{ path('suivi_par_edit', {'IDSuiviPar': suivi_par.IDSuiviPar}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="3">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>

	<a href="{{ path('suivi_par_new') }}">Create new</a>
{% endblock %}
